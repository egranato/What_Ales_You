<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

{{#if users}}
  <table>
    <tr>
      <th>ID</th>
      <th>User Name</th>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
    {{#each users}}
    <tr>
        <td>{{id}}</td>
        <td>{{username}}</td>
        <td>{{email}}</td>
        <td>{{first_name}}</td>
        <td>{{last_name}}</td>
    </tr>
    {{/each}}
  </table>
  <h4><a href="/">Home</a></h4>
{{else}}
    <p>No users!</p>
    <h4><a href="/">Home</a></h4>
    <h4><a href="/logout">Logout</a></h4>
{{/if}}
